var cjs=new Object;cjs.canvas={},cjs.stage={},cjs.exportRoot={},define(["view/content"],function(a){var b=a.extend({canvas:null,stage:null,exportRoot:null,initialize:function(){},error:function(){alert("共通エラーページ")},removeEvent:function(){this.stage&&this.stage.removeAllEventListeners("tick")},removeCjs:function(){this.off(),this.undelegateEvents(),this.stopListening(),this.removeEvent()}});return b});